<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="26.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ControllerMenuBar">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
                  <MenuItem mnemonicParsing="false" onAction="#logout" text="Log Out" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
                  <MenuItem mnemonicParsing="false" onAction="#support" text="Contact Support" />
                  <MenuItem mnemonicParsing="false" onAction="#feedback" text="Provide Feedback" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="31.0" prefWidth="1200.0">
         <children>
            <Button fx:id="buttonEffortConsole" mnemonicParsing="false" onMouseClicked="#goToEffortConsole" prefHeight="45.0" prefWidth="200.0" stylesheets="@application.css" text="Effort Console" />
            <Button fx:id="buttonEffortLogEditor" mnemonicParsing="false" onMouseClicked="#goToEffortLogEditor" prefHeight="45.0" prefWidth="200.0" stylesheets="@application.css" text="Effort Log Editor" />
            <Button fx:id="buttonDefectConsole" mnemonicParsing="false" onMouseClicked="#goToDefectConsole" prefHeight="45.0" prefWidth="200.0" stylesheets="@application.css" text="Defect Console" />
            <Button fx:id="buttonLogs" mnemonicParsing="false" onMouseClicked="#goToLogs" prefHeight="45.0" prefWidth="200.0" stylesheets="@application.css" text="Logs" />
            <Button fx:id="buttonDefinitions" mnemonicParsing="false" onMouseClicked="#goToDefinitions" prefHeight="45.0" prefWidth="200.0" stylesheets="@application.css" text="Definitions" />
            <Button fx:id="buttonLogManager" mnemonicParsing="false" onMouseClicked="#goToLogManager" prefHeight="45.0" prefWidth="200.0" stylesheets="@application.css" text="LogManager" />
            <Button fx:id="buttonPlanningPoker" mnemonicParsing="false" onMouseClicked="#goToPlanningPoker" prefHeight="45.0" prefWidth="200.0" stylesheets="@application.css" text="Planning Poker" />
         </children>
      </HBox>
   </children>
</VBox>
